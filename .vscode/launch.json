{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Run plotting.plot",
            "type": "python",
            "request": "launch",
            "module": "plotting.plot",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/imit_ucb",
            "env": {
                "PYTHONPATH": "${workspaceFolder}:imit_ucb:${env:PYTHONPATH}"
            },
            "args": ["--noiseE", "0.0"]
        }
    ]
}
